        <div class="user-block">

            <img class="img-circle img-bordered-sm" src="~/AdminLTE-3.0.4/dist/img/avatar3.png" alt="user image">

            <span class="username">

                <a href="#">@x.KATEGORI</a>

                <a href="#" class="float-right btn-tool"><i class="fas fa-times"></i></a>

            </span>

            <span class="description">@x.TARIH.ToString()</span>

        </div>

        <!-- /.user-block -->

        <p>
            @x.ICERIK
        </p>



        <p>

            <a href="#" class="link-black text-sm mr-2"></a>

            <a href="#" class="link-black text-sm"></a>


        </p>



       
    </div>
}